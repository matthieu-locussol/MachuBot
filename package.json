{
   "name": "machubot",
   "version": "0.7.2",
   "description": "Yet another discord bot.",
   "repository": "git@github.com:matthieu-locussol/MachuBot.git",
   "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
      "build": "tsc",
      "start": "node build/src/main.js",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
      "check:eslint": "eslint --ignore-path .gitignore src/**/*",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "format": "pretty-quick --staged",
      "tests": "TZ='Europe/Paris' jest",
      "tests:ci": "TZ='Europe/Paris' jest --ci",
      "release": "standard-version -a",
      "postrelease": "git add CHANGELOG.md ; git commit -m\"chore: updated CHANGELOG.md\"",
      "postinstall": "husky install"
   },
   "dependencies": {
      "@discord-player/extractor": "^4.2.2",
      "@discordjs/opus": "^0.9.0",
      "@discordjs/voice": "^0.16.0",
      "axios": "^1.4.0",
      "chart.js": "3.8.0",
      "chartjs-node-canvas": "^4.1.6",
      "discord-player": "^6.3.0",
      "discord.js": "^14.11.0",
      "dotenv": "^16.0.3",
      "download": "^8.0.0",
      "keyv": "^4.5.2",
      "luxon": "^3.3.0",
      "rosu-pp": "^0.9.4",
      "winston": "^3.8.2",
      "winston-daily-rotate-file": "^4.7.1",
      "ytdl-core": "git+ssh://git@github.com:khlevon/node-ytdl-core.git#v4.11.4-patch.2"
   },
   "devDependencies": {
      "@commitlint/cli": "^17.6.3",
      "@commitlint/config-conventional": "^17.6.3",
      "@keyv/sqlite": "^3.6.5",
      "@types/download": "^8.0.2",
      "@types/jest": "^29.5.1",
      "@types/keyv": "^4.2.0",
      "@types/luxon": "^3.3.0",
      "@types/node": "^20.2.0",
      "@typescript-eslint/eslint-plugin": "^5.59.6",
      "@typescript-eslint/parser": "^5.59.6",
      "cross-env": "^7.0.3",
      "eslint": "^8.40.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "node-fetch": "^3.3.1",
      "prettier": "^2.8.8",
      "pretty-quick": "^3.1.3",
      "standard-version": "^9.5.0",
      "ts-jest": "^29.1.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.0.4"
   }
}
