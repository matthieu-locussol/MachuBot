{
   "name": "machubot",
   "version": "0.6.0",
   "description": "Yet another discord bot.",
   "repository": "git@github.com:matthieu-locussol/MachuBot.git",
   "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
      "build": "tsc",
      "start": "node build/src/main.js",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
      "check:eslint": "eslint --ignore-path .gitignore src/**/*",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "tests": "jest",
      "tests:ci": "jest --ci",
      "release": "standard-version -a",
      "postrelease": "git add CHANGELOG.md ; git commit -m\"chore: updated CHANGELOG.md\"",
      "postinstall": "husky install"
   },
   "lint-staged": {
      "*.{ts,tsx,d.ts}": [
         "tsc-files --noEmit",
         "yarn check:eslint"
      ]
   },
   "dependencies": {
      "@discordjs/builders": "^0.15.0",
      "@discordjs/rest": "^0.5.0",
      "axios": "^0.27.2",
      "chart.js": "^3.8.0",
      "chartjs-node-canvas": "^4.1.6",
      "discord-api-types": "^0.36.1",
      "discord.js": "^13.8.1",
      "dotenv": "^16.0.1",
      "download": "^8.0.0",
      "keyv": "^4.3.2",
      "luxon": "^3.0.1",
      "rosu-pp": "^0.7.1",
      "winston": "^3.8.1",
      "winston-daily-rotate-file": "^4.7.1"
   },
   "devDependencies": {
      "@commitlint/cli": "^17.0.3",
      "@commitlint/config-conventional": "^17.0.3",
      "@keyv/sqlite": "^3.5.2",
      "@types/download": "^8.0.1",
      "@types/jest": "^28.1.5",
      "@types/keyv": "^3.1.4",
      "@types/luxon": "^2.3.2",
      "@types/node": "^18.0.4",
      "@typescript-eslint/eslint-plugin": "^5.30.6",
      "@typescript-eslint/parser": "^5.30.6",
      "cross-env": "^7.0.3",
      "eslint": "^8.19.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "husky": "^8.0.1",
      "jest": "^28.1.3",
      "lint-staged": "^13.0.3",
      "node-fetch": "^3.2.8",
      "prettier": "^2.7.1",
      "pretty-quick": "^3.1.3",
      "standard-version": "^9.5.0",
      "ts-jest": "^28.0.6",
      "ts-node-dev": "^2.0.0",
      "tsc-files": "^1.1.3",
      "typescript": "^4.7.4"
   }
}
