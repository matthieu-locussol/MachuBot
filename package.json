{
   "name": "machubot",
   "version": "0.1.0",
   "description": "Yet another discord bot.",
   "repository": "git@github.com:matthieu-locussol/MachuBot.git",
   "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
      "build": "tsc",
      "start": "node build/main.js",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx}\"",
      "check:eslint": "eslint --ignore-path .gitignore src/**/*",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "tests": "jest",
      "tests:ci": "jest --ci"
   },
   "pre-commit": [
      "check:typescript",
      "check:eslint",
      "check:prettier"
   ],
   "dependencies": {
      "@discordjs/builders": "^0.8.2",
      "@discordjs/rest": "^0.1.0-canary.0",
      "axios": "^0.24.0",
      "discord-api-types": "^0.24.0",
      "discord.js": "^13.3.1",
      "dotenv": "^10.0.0",
      "winston": "^3.3.3",
      "winston-daily-rotate-file": "^4.5.5"
   },
   "devDependencies": {
      "@types/jest": "^27.0.2",
      "@types/node": "^16.11.7",
      "@typescript-eslint/eslint-plugin": "^5.3.1",
      "@typescript-eslint/parser": "^5.3.1",
      "cross-env": "^7.0.3",
      "eslint": "^8.2.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-typescript": "^15.0.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "jest": "^27.3.1",
      "node-fetch": "^3.1.0",
      "pre-commit": "^1.2.2",
      "prettier": "^2.4.1",
      "ts-jest": "^27.0.7",
      "ts-node-dev": "^1.1.8",
      "typescript": "^4.4.4"
   }
}
